{
  "name": "toml-eslint-parser",
  "version": "0.1.1",
  "description": "A TOML parser that produces output compatible with ESLint",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "setup": "git submodule update --init",
    "prebuild": "npm run -s clean",
    "build": "tsc --project ./tsconfig.build.json",
    "clean": "rimraf .nyc_output lib coverage",
    "lint": "eslint \"tests\" \"src\" --ext .js,.ts",
    "eslint-fix": "npm run lint -- --fix",
    "test": "env-cmd -e tz mocha --require ts-node/register \"tests/src/**/*.ts\" --reporter dot --timeout 60000",
    "cover": "nyc --reporter=lcov npm run test",
    "debug": "env-cmd -e tz mocha --require ts-node/register/transpile-only --inspect \"tests/src/**/*.ts\" --reporter dot",
    "preversion": "npm run lint && npm test",
    "update-fixtures": "env-cmd -e tz ts-node --transpile-only ./tools/update-fixtures.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ota-meshi/toml-eslint-parser.git"
  },
  "keywords": [
    "toml",
    "eslint",
    "parser"
  ],
  "author": "Yosuke Ota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ota-meshi/toml-eslint-parser/issues"
  },
  "homepage": "https://github.com/ota-meshi/toml-eslint-parser#readme",
  "dependencies": {
    "eslint-visitor-keys": "^1.3.0"
  },
  "devDependencies": {
    "@iarna/toml": "^3.0.0",
    "@ota-meshi/eslint-plugin": "^0.0.10",
    "@types/eslint": "^7.2.0",
    "@types/eslint-visitor-keys": "^1.0.0",
    "@types/js-yaml": "^4.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jsonc": "^0.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-regexp": "^0.1.1",
    "eslint-plugin-vue": "^7.2.0",
    "js-yaml": "^4.0.0",
    "mocha": "^7.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "vue-eslint-parser": "^7.2.0"
  }
}
